{
    "name": "Yet Another LeetCode Timer",
    "description": "Integrated LeetCode extension with times stored locally",
    "version": "1.1.0",
    "manifest_version": 3,
    "permissions": [
        "storage"
    ],
    "action": {
        "default_icon": {
            "16": "/img/ext16.png",
            "32": "/img/ext32.png",
            "128": "/img/ext128.png"
        }
    },
    "icons": {
        "16": "/img/ext16.png",
        "32": "/img/ext32.png",
        "128": "/img/ext128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://leetcode.com/problems/*"
            ],
            "run_at": "document_start",
            "js": [
                "src/main.js",
                "src/timer.js"
            ]
        }
    ],
    "background": {
        "service_worker": "src/background.js"
    }
}